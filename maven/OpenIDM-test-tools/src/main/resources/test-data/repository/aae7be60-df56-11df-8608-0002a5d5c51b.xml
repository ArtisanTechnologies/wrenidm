                <c:operation>add</c:operation>
                <c:order>after</c:order>
                <c:host>connector</c:host>
                <c:language>sh</c:language>
                <c:argument>
                    <c:valueExpression>
                        declare namespace i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd";
                        declare namespace unix="http://openidm.forgerock.com/xml/ns/public/experiments/unix-1.xsd";
                        $c:user/c:extension/unix:homedir
                    </c:valueExpression>
                    <c:name>HOMEDIR</c:name>
                </c:argument>
                <c:code>mkdir $HOMEDIR</c:code>
            </c:script>
    </c:scripts>

</c:object>
