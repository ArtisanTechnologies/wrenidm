                <iccldap:jdbcUrlTemplate xmlns:iccldap="http://openidm.forgerock.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.databasetable/org.identityconnectors.databasetable.DatabaseTableConnector/1.1.4958">jdbc:derby:target/testdb</iccldap:jdbcUrlTemplate>
                <iccldap:table xmlns:iccldap="http://openidm.forgerock.com/xml/ns/resource/idconnector/bundle/org.identityconnectors.databasetable/org.identityconnectors.databasetable.DatabaseTableConnector/1.1.4958">account</iccldap:table>
