                                </xsd:element>
                                <xsd:element minOccurs="0" name="description" type="xsd:string">
                                    <xsd:annotation>
                                        <xsd:appinfo>
                                            <r:help>DESCRIPTION_NAME_HELP_KEY</r:help>
                                            <r:nativeAttributeName>description</r:nativeAttributeName>
