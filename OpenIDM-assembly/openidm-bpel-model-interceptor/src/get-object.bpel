<?xml version="1.0" encoding="UTF-8"?>
<process
    name="getObject"
    targetNamespace="http://openidm.forgerock.com/xml/ns/openidm-bpel-model-interceptor/process"
    xmlns:tns="http://openidm.forgerock.com/xml/ns/openidm-bpel-model-interceptor/process"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2">
    <import namespace="http://openidm.forgerock.com/xml/ns/public/model/model-1.wsdl" location="openidm-model/contract/model.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="invokedModel" xmlns:tns="http://openidm.forgerock.com/xml/ns/public/model/model-1.wsdl" partnerLinkType="tns:idmModel" partnerRole="idmModelPortTypeRole"/>
        <partnerLink name="implementedModel" xmlns:tns="http://openidm.forgerock.com/xml/ns/public/model/model-1.wsdl" partnerLinkType="tns:idmModel" myRole="idmModelPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="providedGetObjectOut" xmlns:tns="http://openidm.forgerock.com/xml/ns/public/model/model-1.wsdl" messageType="tns:getObjectResponse"/>
        <variable name="implementedGetObjectIn" xmlns:tns="http://openidm.forgerock.com/xml/ns/public/model/model-1.wsdl" messageType="tns:getObjectRequest"/>
    </variables>
    <sequence>
        <receive name="ReceiveGetObject" createInstance="yes" partnerLink="implementedModel" operation="getObject" xmlns:tns="http://openidm.forgerock.com/xml/ns/public/model/model-1.wsdl" portType="tns:idmModelPortType" variable="implementedGetObjectIn">
            <sxt:trace>
                <sxt:log level="info" location="onComplete">
                    <from>concat('[BPEL] getObject IN oid:
', $implementedGetObjectIn.oid)</from>
                </sxt:log>
            </sxt:trace>
        </receive>
        <invoke name="InvokeGetObject" partnerLink="invokedModel" operation="getObject" xmlns:tns="http://openidm.forgerock.com/xml/ns/public/model/model-1.wsdl" portType="tns:idmModelPortType" inputVariable="implementedGetObjectIn" outputVariable="providedGetObjectOut"/>
        <reply name="ReplyGetObject" partnerLink="implementedModel" operation="getObject" xmlns:tns="http://openidm.forgerock.com/xml/ns/public/model/model-1.wsdl" portType="tns:idmModelPortType" variable="providedGetObjectOut"/>
    </sequence>
</process>




