    <schemaHandling>
        <accountType default="true">
            <name>Default Account</name>
            <objectClass xmlns:ri="http://openidm.forgerock.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2">ri:Account</objectClass>
            <attribute ref="ids:__NAME__" xmlns:ids="http://openidm.forgerock.com/xml/ns/public/resource/idconnector/resource-schema-1.xsd">
                <name>Distinguished Name</name>
                <access>create</access>
                <access>read</access>
                <outbound default="true">
                    <i:valueExpression xmlns:c="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd" xmlns:ri="http://openidm.forgerock.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" xmlns:i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd">
                            declare namespace i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd";
                            concat('uid=', $i:user/c:name, ',ou=people,dc=example,dc=com')
                            </i:valueExpression>
                </outbound>
            </attribute>
            <attribute ref="ids:__UID__" xmlns:ids="http://openidm.forgerock.com/xml/ns/public/resource/idconnector/resource-schema-1.xsd">
                <name>Entry UUID</name>
                <access>read</access>
            </attribute>
            <attribute ref="ri:cn" xmlns:ri="http://openidm.forgerock.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2">
                <name>Common Name</name>
                <access>create</access>
                <access>read</access>
                <access>update</access>
                <outbound>
                    <i:valueExpression xmlns:c="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd" xmlns:ids="http://openidm.forgerock.com/xml/ns/public/resource/idconnector/resource-schema-1.xsd" xmlns:i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd">
                            declare namespace i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd";
                            $i:user/i:fullName
