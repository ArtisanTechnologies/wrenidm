                            declare namespace i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd";
                            $i:user/i:familyName
                        </i:valueExpression>
                </outbound>
                <i:inbound xmlns:c="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd" xmlns:ids="http://openidm.forgerock.com/xml/ns/public/resource/idconnector/resource-schema-1.xsd" xmlns:i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd">
                    <c:target>
                            declare namespace i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd";
                            $i:user/i:familyName
                        </c:target>
            </i:inbound>
            </attribute>
            <attribute ref="ri:givenName" xmlns:ri="http://openidm.forgerock.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2">
                <name>Given Name</name>
                <access>create</access>
                <access>read</access>
                <access>update</access>
                <outbound>
                    <i:valueExpression xmlns:c="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd" xmlns:ids="http://openidm.forgerock.com/xml/ns/public/resource/idconnector/resource-schema-1.xsd" xmlns:i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd">
