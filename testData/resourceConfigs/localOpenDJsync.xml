            <attribute ref="ri:sn" xmlns:ri="http://openidm.forgerock.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2">
                <name>Surname</name>
                <access>create</access>
                <access>read</access>
                <access>update</access>
