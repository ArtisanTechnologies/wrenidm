                                <xsd:annotation>
                                    <xsd:appinfo>
                                        <r:nativeAttributeName>givenName</r:nativeAttributeName>
                                        </xsd:appinfo>
                                    </xsd:annotation>
                                </xsd:element>
                            </xsd:sequence>
