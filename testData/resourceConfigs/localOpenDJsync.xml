    </schemaHandling>
    <synchronization>
        <enabled>true</enabled>
        <pollingInterval>60</pollingInterval>
        <correlation>
            <c:equal xmlns:c="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd" xmlns:ids="http://openidm.forgerock.com/xml/ns/public/resource/idconnector/resource-schema-1.xsd" xmlns:ri="http://openidm.forgerock.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" xmlns:i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd">
                <i:path>.</i:path>
                <i:valueExpression ref="c:name">
                        declare namespace i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd";
                        declare namespace dj="http://openidm.forgerock.com/xml/ns/samples/localhostOpenDJ";
                        $i:account/i:attributes/dj:uid
                        </i:valueExpression>
         </c:equal>
        </correlation>
        <reaction>
            <situation>confirmed</situation>
            <action ref="http://openidm.forgerock.com/xml/ns/public/model/action-1#modifyUser"/>
        </reaction>
        <reaction>
            <situation>deleted</situation>
            <action ref="http://openidm.forgerock.com/xml/ns/public/model/action-1#unlinkAccount"/>
        </reaction>
        <reaction>
            <situation>missing</situation>
            <action ref="http://openidm.forgerock.com/xml/ns/public/model/action-1#unlinkAccount"/>
        </reaction>
        <reaction>
            <situation>found</situation>
            <action ref="http://openidm.forgerock.com/xml/ns/public/model/action-1#linkAccount"/>
        </reaction>
        <reaction>
            <situation>unassigned</situation>
            <action ref="http://openidm.forgerock.com/xml/ns/public/model/action-1#linkAccount"/>
        </reaction>
        <reaction>
            <situation>unmatched</situation>
            <action ref="http://openidm.forgerock.com/xml/ns/public/model/action-1#addUser">
                <i:userTemplateRef oid="c0c010c0-d34d-b33f-f00d-777111111111" xmlns:c="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd" xmlns:ids="http://openidm.forgerock.com/xml/ns/public/resource/idconnector/resource-schema-1.xsd" xmlns:ri="http://openidm.forgerock.com/xml/ns/public/resource/instances/ef2bc95b-76e0-48e2-86d6-3d4f02d3e1a2" xmlns:i="http://openidm.forgerock.com/xml/ns/public/common/common-1.xsd"/>
            </action>
            <action ref="http://openidm.forgerock.com/xml/ns/public/model/action-1#linkAccount"/>
        </reaction>
    </synchronization>
</object>

